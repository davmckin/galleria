<h1>Add Your Photo!</h1>

<div class="container">
  <div class="row">
    <div class="col-xs-4">
      <%= simple_form_for @photo, html: {class: "form-horizontal"} do |f| %>

        <%= f.input :url %>
        <%= f.input :caption %>
        <%= f.button :submit %>

      <% end %>
    </div>
  </div>
</div>
